<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" prefHeight="380.0"
            style="-fx-background-color: rgba(0, 0, 0, 0.0);" xmlns="http://javafx.com/javafx/8"
            fx:controller="com.johannes.lsctic.FXMLController">
    <children>
      <TabPane fx:id="tabPane" focusTraversable="false" prefHeight="380.0" side="LEFT" style="-fx-background-color: rgba(0, 0, 0, 0.0);" tabClosingPolicy="UNAVAILABLE">
        <tabs>
            <Tab fx:id="intern" text="Interns">
              <content>
                <AnchorPane prefHeight="380.0">
                     <children>
                        <VBox minWidth="220.0" prefHeight="380.0">
                           <children>
                               <TextField fx:id="paneATextIn" promptText="Search....." styleClass="textfield-search"/>
                              <ScrollPane fx:id="scrollPaneA" hbarPolicy="NEVER" hmax="0.0" prefViewportHeight="380.0" prefViewportWidth="300.0" style="-fx-background-color: #FFFFFF11;" vbarPolicy="NEVER">
                                 <content>
                                     <VBox fx:id="panelA" layoutX="29.0" layoutY="79.0" prefHeight="360.0"
                                           prefWidth="300.0" style="-fx-background-color: rgba(38, 45, 70, 0.58);"
                                           AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

                                    </VBox>
                                 </content>
                              </ScrollPane>
                           </children>
                           <padding>
                              <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                           </padding>
                        </VBox>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
          <Tab fx:id="ldap" text="Address">
            <content>
              <AnchorPane prefHeight="380.0">
                     <children>
                        <VBox prefHeight="380.0">
                           <children>
                              <TextField fx:id="paneBTextIn" promptText="Search....." VBox.vgrow="NEVER" styleClass="textfield-search" />
                              <ScrollPane hbarPolicy="NEVER" hmax="0.0" prefViewportHeight="380.0" prefViewportWidth="300.0" style="-fx-background-color: #FFFFFF11;" vbarPolicy="NEVER">
                                 <content>
                                    <VBox fx:id="panelB" layoutX="65.0" layoutY="122.0" prefHeight="380.0" prefWidth="300.0" style="-fx-background-color: rgba(38, 45, 70, 0.58);" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    </VBox>
                                 </content>
                              </ScrollPane>
                           </children>
                           <padding>
                              <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                           </padding>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="historie" text="History">
            <content>
              <AnchorPane prefHeight="380.0">
                     <children>
                        <VBox prefHeight="380.0">
                           <children>
                              <TextField fx:id="paneCTextIn" promptText="Search....." VBox.vgrow="NEVER" styleClass="textfield-search" />
                              <ScrollPane hbarPolicy="NEVER" hmax="0.0" prefViewportHeight="380.0" prefViewportWidth="300.0" vbarPolicy="NEVER">
                                 <content>
                                    <VBox prefHeight="380.0" prefWidth="300.0" style="-fx-background-color: rgba(38, 45, 70, 0.58);">
                                       <children>
                                          <ScrollPane hbarPolicy="NEVER" hmax="0.0" prefViewportHeight="380.0" prefViewportWidth="300.0" style="-fx-background-color: #FFFFFF11;" vbarPolicy="NEVER" VBox.vgrow="ALWAYS">
                                             <content>
                                                <VBox fx:id="panelC" layoutX="58.0" layoutY="66.0" prefHeight="380.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

                                                </VBox>
                                             </content>
                                          </ScrollPane>

                                          <HBox prefWidth="200.0" >
                                             <children>
                                                <Button mnemonicParsing="false" disable="true" text="Last" styleClass="button-ui" />
                                                <HBox HBox.hgrow="ALWAYS" />
                                                <Button alignment="CENTER_RIGHT" contentDisplay="CENTER"  mnemonicParsing="false" text="Next" styleClass="button-ui"/>
                                             </children>
                                              <padding>
                                                  <Insets bottom="5.0" left="3.0" right="3.0" top="5.0" />
                                              </padding>
                                             <VBox.margin>
                                                <Insets />
                                             </VBox.margin>
                                          </HBox>
                                       </children>
                                       <VBox.margin>
                                          <Insets top="1.0" />
                                       </VBox.margin>

                                    </VBox>
                                 </content>
                              </ScrollPane>
                           </children>
                           <padding>
                              <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                           </padding>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab fx:id="optionen" text="Options">
              <content>
                  <VBox>
                     <children>
                        <ScrollPane hbarPolicy="NEVER" prefViewportHeight="380.0" prefViewportWidth="300.0" style="-fx-background-color: rgba(0, 0, 0, 0.0);" vbarPolicy="NEVER">
                           <content>
                              <VBox fx:id="panelD" layoutX="65.0" layoutY="62.0" prefHeight="380.0" prefWidth="300.0" spacing="3.0" style="-fx-background-color: rgba(38, 45, 70, 0.58);" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

                              </VBox>
                           </content>
                        </ScrollPane>
                        <HBox prefWidth="200.0" styleClass="button-hbox">
                           <children>
                              <Button fx:id="optionAccept" mnemonicParsing="false" text="Accept" styleClass="button-ui" />
                              <HBox HBox.hgrow="ALWAYS" />
                              <Button fx:id="optionReject" mnemonicParsing="false" text="Reset" styleClass="button-ui" />
                           </children>
                           <padding>
                              <Insets bottom="5.0" left="3.0" right="3.0" top="5.0" />
                           </padding>
                            <VBox.margin>
                                <Insets left="1.0" right="1.0" />
                            </VBox.margin>
                        </HBox>
                     </children>
                     <padding>
                        <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                     </padding>
                  </VBox>
              </content>
            </Tab>
        </tabs>
      </TabPane>
    </children>
</AnchorPane>
